<div id="page-content">
  <div class="content-header">
    <div class="header-section">
      <h1>
        <%= "#{action_name} #{form_name}" %><br />
      </h1>
    </div>
  </div>
  <ul class="breadcrumb breadcrumb-top">
  <li><%= link_to "Dashboard", backend_path %></li>
  <li><%= "#{action_name} #{form_name}" %></li>
  </ul>
  <div class="row">
    <div class="col-md-12">
      <div class="block">
        <div class="block-title">
          <h2><strong><%= "#{form_name}" %></strong></h2>
        </div>
          <%= form_for [:backend, resource], method: form_method, html: {class: "form-horizontal"} do |f| %>
            <%= render partial: 'form', locals: {f: f} %>
          <% end %>
      </div>
    </div>
  </div>
</div>